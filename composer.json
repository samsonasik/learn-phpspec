{
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\App\\": "spec/App/"
        }
    },
    "require": {
        "php": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "ciaranmcnulty/phpspec-typehintedmethods": "^3.0",
        "leanphp/phpspec-code-coverage": "dev-phpspec-4",
        "phpspec/nyan-formatters": "dev-master",
        "phpspec/phpspec": "^4.0",
        "satooshi/php-coveralls": "^1.0"
    },
     "repositories" : [
        {
            "type" : "vcs",
            "url" : "git@github.com:Sam-Burns/phpspec-code-coverage.git"
        }
    ]
}
